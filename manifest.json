{
    "name": "ImageExtractor",
    "description": "Extracts image urls",
    "version": "1.0",
    "manifest_version": 2,
    "web_accessible_resources": [
        "script.js"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "permissions": [
        "storage",
        "activeTab",
        "tabs",
        "webRequest",
        "webRequestBlocking",
        "downloads"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "default_icon": "devian.png",
        "default_popup": "popup.html"
    }
}
